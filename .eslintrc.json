{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": ["error", "double"],
    "react/prop-types": "off",
    "react/jsx-key": "off",
    "no-case-declarations": "off",
    "no-prototype-builtins": "off"
  }
}
